{
  "name": "data-analyzer",
  "version": "1.1.0",
  "private": false,
  "homepage": "https://dataanalyzer.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/justsml/DataAnalyzer.app"
  },
  "engines": {
    "node": "< 14"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/recoil": "0.0.1",
    "apexcharts": "^3.23.1",
    "clipboard-copy": "^4.0.1",
    "comlink": "^4.3.0",
    "csv-parse": "^4.15.0",
    "debug": "^4.3.1",
    "functional-promises": "^2.0.2",
    "github-markdown-css": "^4.0.0",
    "handlebars": "^4.7.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.isdate": "^4.0.1",
    "lodash.isnumber": "^3.0.3",
    "lodash.snakecase": "^4.1.1",
    "marked": "^2.0.0",
    "node-sass": "^4.14.1",
    "notistack": "^1.0.3",
    "react": "^17.0.1",
    "react-apexcharts": "^1.3.7",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.2",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-syntax-highlighter": "^15.4.3",
    "redux": "^4.0.5",
    "remark-gfm": "^1.0.0",
    "serve": "^11.3.2",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "PUBLIC_URL=https://dataanalyzer.app react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "npx prettier -w \"{app,src,__{tests,mocks}__}/**/*.{ts,js}\"",
    "postinstall": "cd src/schema-analyzer && npm install"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jest": true
    },
    "plugins": [
      "import",
      "node",
      "promise",
      "prettier"
    ],
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/marked": "^1.2.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "2.2.1",
    "react-app-rewire-alias": "^0.2.1",
    "react-app-rewired": "^2.1.8"
  },
  "jest": {
    "transformIgnorePatterns": []
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslint": {
    "rules": {
      "@typescript-eslint/no-redeclare": [
        "off"
      ]
    }
  }
}
