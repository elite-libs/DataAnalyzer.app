{
  "name": "schema-analyzer",
  "version": "2.0.0",
  "description": "Heuristic \"column\" type & size analysis w/ enumeration detection.",
  "type": "commonjs",
  "module": "index.tsx",
  "main": "index.tsx",
  "browser": "dist/schema-analyzer.bundle.js",
  "files": [
    "dist",
    "build/index.d.ts",
    "index.ts"
  ],
  "scripts": {
    "prebuild": "rm -rf dist/*",
    "build": "npm run build:webpack && NODE_ENV=production rollup -c && NODE_ENV=development rollup -c && npm run compress",
    "build:webpack": "webpack -c webpack.config.js",
    "coverage": "jest --coverage --maxWorkers 4 && codecov -f coverage/coverage-final.json",
    "postcoverage": "codecov --token=$CODECOV_TOKEN",
    "dev": "rollup -c -w",
    "test": "jest --runTestsByPath --coverage --maxWorkers 4 ./index.test.ts ./utils/type-helpers.test.ts",
    "test:debug": "node --inspect-brk=9823 ./node_modules/.bin/jest --runTestsByPath ./index.test.ts ./utils/type-*.test.ts",
    "test:watch": "jest --watch",
    "pretest": "npm run build",
    "format": "prettier -w *.js index*.ts utils/*.ts",
    "compress": "zopfli -i2000 --gzip ./dist/schema-analyzer*min.js && brotli --keep --quality=11 -- ./dist/schema-analyzer*min.js"
  },
  "engines": {
    "node": "< 14"
  },
  "types": "./index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justsml/schema-analyzer.git"
  },
  "keywords": [],
  "author": {
    "name": "Dan Levy",
    "url": "https://danlevy.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justsml/schema-analyzer/issues"
  },
  "homepage": "https://github.com/justsml/schema-analyzer#readme",
  "dependencies": {
    "debug": "^4.3.1",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@babel/core": "~7.8.3",
    "@babel/helper-compilation-targets": "~7.12.5",
    "@babel/plugin-transform-runtime": "~7.8.3",
    "@babel/preset-env": "~7.8.3",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime-corejs3": "~7.8.3",
    "@rollup/plugin-commonjs": "~11.0.1",
    "@rollup/plugin-node-resolve": "~7.0.0",
    "@rollup/plugin-typescript": "8.1.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "~25.1.0",
    "@types/lodash-es": "^4.17.4",
    "babel-eslint": "~10.0.3",
    "babel-jest": "26.6.3",
    "codecov": "3.8.1",
    "core-js": "3.8.2",
    "csv-parse": "~4.8.5",
    "eslint-config-standard": "~14.1.0",
    "eslint-config-standard-react": "~9.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "~11.0.0",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-standard": "~4.0.1",
    "jest": "26.6.3",
    "lodash-es": "^4.17.20",
    "prettier": "^2.2.1",
    "rollup": "~1.30.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "~5.2.0",
    "standard": "16.0.3",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "mjs"
    ],
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^lodash-es$": "<rootDir>/node_modules/lodash/index.js"
    }
  },
  "babel": {
    "plugins": [],
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": {
            "version": "3.8.2",
            "proposals": true
          },
          "modules": "auto",
          "useBuiltIns": "usage",
          "targets": {
            "node": 14,
            "browsers": [
              "Chrome >= 60",
              "Safari >= 10.1",
              "iOS >= 10.3",
              "Firefox >= 54",
              "Edge >= 15"
            ]
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jest": true
    },
    "plugins": [
      "import",
      "node",
      "promise",
      "prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "CI=true npm test"
    }
  },
  "browserslist": [
    ">2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "remarkConfig": {
    "settings": {
      "listItemIndent": "mixed"
    },
    "plugins": [
      "remark-stringify",
      [
        "remark-lint-list-item-indent",
        false
      ]
    ]
  }
}
